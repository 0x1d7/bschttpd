{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080",
        "Protocols": "Http1"
      },
      "Https": {
        "Url": "https://localhost:8443",
        "Protocols": "Http2",
        "Certificate": {
          "Path": "/etc/ssl/certs/localhost.pem",
          "KeyPath": "/etc/ssl/private/localhost.key"
        }
      }
    }
  },
  "WebServerConfiguration": {
    "HttpsRedirection": false,
    "Wwwroot": "/srv/wwwroot",
    "DefaultDocument": "index.html",
    "ErrorPagesPath": "errorpages",
    "W3CLogFlushInterval": 300,
    "W3CLogFileSizeLimit": 52428800,
    "W3CLogName": "bschttpd_w3c",
    "W3CLogDirectory": "logs"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=sql/bschttpd.db"
  },
  "ContentConfiguration": {
    "NoServe": [
      "appsettings.json",
      "bschttpd",
      "bschttpd.exe",
      ".dll",
      ".json",
      ".pdb",
      ".db",
      ".pem",
      ".p12",
      ".pfx",
      ".key",
      ".der",
      ".cer"
    ],
    "NoCache": [
      "appsettings.json",
      "bschttpd",
      "bschttpd.exe",
      ".dll",
      ".json",
      ".pdb",
      ".db",
      ".pem",
      ".p12",
      ".pfx",
      ".key",
      ".der",
      ".cer"
    ],
    "ContentTypeMap": {
      ".html": "text/html",
      ".htm": "text/html",
      ".css": "text/css",
      ".js": "application/javascript",
      ".jpg": "image/jpeg",
      ".jpeg": "image/jpeg",
      ".png": "image/png",
      ".ico": "image/x-icon",
      ".svg": "image/svg+xml",
      ".gif": "image/gif",
      ".txt": "text/plain",
      ".md": "text/markdown",
      ".zip": "application/zip",
      ".7z": "application/x-7z-compressed",
      ".doc": "application/msword",
      ".xls": "application/vnd.ms-excel",
      ".ppt": "application/vnd.ms-powerpoint",
      ".pdf": "application/pdf",
      ".docx": "application/word",
      ".xlsx": "application/excel",
      ".pptx": "application/ppt",
      ".eml": "application/eml",
      ".msg": "text/plain",
      ".json": "application/json"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}